.viewImg{
    display: flex;
    flex-direction: column;
    align-items: center;
}
.completeImgBg{
    width: 100%;
    height: 1000rpx;
    position: absolute;
    z-index: -1;
    top: 0;
}
.completeImgAvatar{
    height: 160rpx;
    width: 160rpx;
    padding-top: 100rpx;
}
.formContainer{
    font-size: 32rpx;
    width: 65%;
    margin: 0 auto;
}
.inputView{
    margin: 10rpx 0;
    display: flex;
    flex-direction: column;
}
.inputItem{
    border: 1rpx solid rgb(164, 164, 164);
    border-radius: 30rpx;
    padding: 5rpx 0;
    padding-left: 30rpx;
}
.delivery{
    margin-top: 50rpx;
    height: 80rpx;
    line-height: 80rpx;
    width: 300rpx;
    background-color: white;
    border: 2rpx solid rgb(0, 224, 121);
}
