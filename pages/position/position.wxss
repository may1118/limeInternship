.button-list{
    overflow:hidden;
}
.button-wrapper{
    float: left;
    width:33.3%;
}
.button{
    padding: 15rpx 0;
    margin: 15rpx;
    text-align: center;
    border:solid 5rpx #ccc;
    border-radius : 8rpx;
}
.title{
    line-height: 50rpx;
    background-color :#eee;
    padding: 15rpx;
    color: #666;
    font-size:30rpx;
}
.item{
    line-height:40rpx;
    color: #666;
    padding-left:15rpx;
}
.clear{
    clear:both;
    height:0;
    overflow:hidden;
}
.border-bottom{
    border-color: #ccc;
    line-height:60rpx;
    color: #666;
    padding-left:10rpx;
}
