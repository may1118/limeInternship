viewItem.viewContainer{
    font-size: 32rpx;
}
.viewTop{
    background-color: rgb(0, 224, 121);
    height: 300rpx;
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
}
.viewTopFont{
    padding-top: 110rpx;
    font-size: 50rpx;
}
.viewTopImgContainer{
    height: 150rpx;
    width: 150rpx;
    margin-top: 50rpx;
}
.viewTopImg{
    height: 100%;
    width: 100%;
}
.viewItem{
    display: flex;
    flex-direction: column;
    margin-top: 150rpx;
}
.viewItemContainer{
    display: flex;
    height: 100rpx;
    align-items: center;
    font-size: 32rpx;
    padding: 0 20rpx;
    border-bottom: 2rpx solid rgb(228, 228, 228);
}
.viewItemIcon{
    height: 45rpx;
    width: 45rpx;
    padding: 0 15rpx;
}
.viewItemRightIcon{
    width: 12rpx;
    height: 20rpx;
    margin-left: 63%;
}
